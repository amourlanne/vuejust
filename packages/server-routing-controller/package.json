  "dependencies": {
    "@types/connect-redis": "^0.0.11",
    "@types/express-session": "^1.15.14",
    "@types/i18n": "^0.8.6",
    "@types/redis": "^2.8.14",
    "@types/uuid": "^3.4.5",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "helmet": "^3.18.0",
    "i18n": "^0.8.3",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./tests",
    "build": "webpack --config config/webpack/production.config.js",
    "start": "ts-node-dev src/server.ts",
    "test": "jest ./tests",
    "serve": "ts-node src/server.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "fixtures": "ts-node ./node_modules/typeorm-fixtures-cli/dist/cli.js ./src/fixtures --config ormconfig.json --sync"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread",
      "@babel/plugin-transform-classes",
      "@babel/plugin-proposal-export-default-from"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "prettier",
      "prettier/@typescript-eslint"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testEnvironment": "node",
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfigFile": "tsconfig.json"
      }
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --ext .js,.jsx,.ts,.tsx ./src ./tests",
      "git add"
    ]
  }
}
